{
  "name": "express-preact",
  "version": "0.0.1",
  "description": "Starting point for Express Preact projects",
  "main": "server.js",
  "author": "Guy Lyons",
  "license": "MIT",
  "repository": "git@github.com:guylyons/express-react.git",
  "scripts": {
    "start": "concurrently \"npm run serve\" \"npm run bundle-serve\"",
    "serve": "./node_modules/babel-cli/bin/babel-node.js server.js --presets es2015,stage-2",
    "bundle": "webpack --config webpack.config.js --progress",
    "build": "webpack --config webpack.production.js --progress",
    "bundle-serve": "webpack-dev-server --open"
  },
  "dependencies": {
    "express": "^4.16.2",
    "express-favicon": "^2.0.0",
    "preact": "^8.2.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.17.0"
  }
}
