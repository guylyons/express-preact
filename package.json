{
  "name": "express-react",
  "version": "1.1.0",
  "description": "Starting point for Express React projects",
  "main": "server.js",
  "author": "Guy Lyons",
  "license": "MIT",
  "repository": "git@github.com:guylyons/express-react.git",
  "scripts": {
    "start": "concurrently \"npm run serve\" \"npm run bundle-serve\"",
    "serve": "./node_modules/babel-cli/bin/babel-node.js server.js --presets es2015,stage-2",
    "bundle": "webpack --config webpack.config.js",
    "bundle-serve": "webpack-dev-server --open"
  },
  "dependencies": {
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "express-favicon": "^2.0.0",
    "lodash": "^4.17.4",
    "material-design-lite": "^1.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.17.0",
    "webpack-dev-server": "^2.4.5"
  }
}
